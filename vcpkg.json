    "libpqxx",
    "hiredis",
    { "name": "redis-plus-plus", "features": ["cxx17"] },
    "spdlog",
    "argparse",
    "mailio",
    "magic-enum",

    "boost-filesystem",
    "boost-thread",
    "boost-chrono",
    "boost-random",
